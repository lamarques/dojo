{
  "name": "dojo/php-template",
  "description": "Template para um dojo de desenvolvimento em PHP com PHPUnit",
  "type": "project",
  "license": "MIT",
  "version": "1.0.0",
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Dojo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dojo\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "require": {},
  "scripts": {
    "test": "vendor/bin/phpunit --configuration phpunit.xml"
  }
}
